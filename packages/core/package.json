{
  "name": "@swagent/core",
  "version": "0.1.0",
  "description": "AI-first API documentation generator. Converts OpenAPI specs into token-optimized llms.txt, human-readable markdown, and semantic HTML.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "swagger",
    "llms-txt",
    "api-docs",
    "ai-first",
    "documentation"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/24labs/swagent.git",
    "directory": "packages/core"
  },
  "homepage": "https://swagent.dev"
}
