{
  "name": "swagent",
  "version": "0.1.0",
  "description": "CLI tool for generating AI-first API documentation from OpenAPI specs.",
  "type": "module",
  "bin": {
    "swagent": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "swagger",
    "llms-txt",
    "api-docs",
    "ai-first",
    "documentation",
    "cli",
    "generator"
  ],
  "license": "MIT",
  "dependencies": {
    "@swagent/core": "workspace:*",
    "yaml": "^2.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/X24Labs/SWAgent.git",
    "directory": "packages/cli"
  },
  "homepage": "https://swagent.dev"
}
