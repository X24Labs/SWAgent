{
  "name": "@swagent/hono",
  "version": "0.1.0",
  "description": "Swagent middleware for Hono. Auto-generates /llms.txt, /to-humans.md, and HTML landing from your OpenAPI spec.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "dependencies": {
    "@swagent/core": "workspace:*"
  },
  "peerDependencies": {
    "hono": "^4.12.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/X24Labs/SWAgent.git",
    "directory": "packages/hono"
  },
  "homepage": "https://swagent.dev"
}
