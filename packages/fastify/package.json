{
  "name": "@swagent/fastify",
  "version": "0.1.0",
  "description": "Swagent plugin for Fastify. Auto-generates /llms.txt, /to-humans.md, and HTML landing from your OpenAPI spec.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "swagger",
    "llms-txt",
    "api-docs",
    "ai-first",
    "documentation",
    "fastify",
    "fastify-plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "@swagent/core": "workspace:*",
    "fastify-plugin": "^5.1.0"
  },
  "peerDependencies": {
    "fastify": ">=4.0.0"
  },
  "devDependencies": {
    "@fastify/swagger": "^9.7.0",
    "fastify": "^5.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/X24Labs/SWAgent.git",
    "directory": "packages/fastify"
  },
  "homepage": "https://swagent.dev"
}
