{
  "name": "@swagent/nestjs",
  "version": "0.1.0",
  "description": "Swagent module for NestJS. Auto-generates /llms.txt, /to-humans.md, and HTML landing from your OpenAPI spec.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "swagger",
    "llms-txt",
    "api-docs",
    "ai-first",
    "documentation",
    "nestjs",
    "module"
  ],
  "license": "MIT",
  "dependencies": {
    "@swagent/core": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@swc/core": "^1.15.11",
    "@types/supertest": "^6.0.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "supertest": "^7.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/X24Labs/SWAgent.git",
    "directory": "packages/nestjs"
  },
  "homepage": "https://swagent.dev"
}
